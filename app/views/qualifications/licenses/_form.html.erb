<%= form_for([:qualifications, @qualifications_license]) do |f| %>

  <%= render partial: "/shared/form_errors", locals: { obj: @qualifications_license, name: "license" } %>

  <fieldset>
    <div class="actions">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
  </fieldset>

  <fieldset>
    <div class="actions">
      <%= f.submit nil, class: "btn btn-primary" %>
    </div>
  </fieldset>

<% end %>
