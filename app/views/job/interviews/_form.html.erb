<%= form_for([:job, @job_interview]) do |f| %>

  <%= render partial: "/shared/form_errors", locals: { obj: @job_interview, name: "job interview" } %>

  <fieldset>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </fieldset>

  <fieldset>
    <div class="actions">
      <%= f.submit nil, class: "btn btn-primary" %>
    </div>
  </fieldset>

<% end %>
